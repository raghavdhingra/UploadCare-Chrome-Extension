{
  "short_name": "Uploadcare",
  "name": "Uploadcare | File uploader",
  "manifest_version": 3,
  "version": "0.1.2",
  "description": "A browser extension that allows you to upload content to the cloud and get a url link to share or use in developing applications.",
  "icons": {
    "16": "upload-care-16.png",
    "32": "upload-care-32.png",
    "48": "upload-care-48.png",
    "64": "upload-care-64.png",
    "128": "upload-care-128.png",
    "256": "upload-care.png"
  },
  "action": {
    "default_icon": "upload-care.png",
    "default_popup": "index.html",
    "default_title": "Uploadcare"
  },
  "background": {},
  "permissions": ["storage"]
}
